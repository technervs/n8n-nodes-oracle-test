{
	"name": "n8n-nodes-oracle-test",
	"version": "1.0.0",
	"description": "Simple Oracle Test node for n8n credential testing bug reproduction",
	"keywords": [
		"n8n-community-node-package"
	],
	"license": "MIT",
	"homepage": "",
	"author": {
		"name": "Test User",
		"email": "test@example.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/test/n8n-nodes-oracle-test.git"
	},
	"main": "index.js",
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"format": "prettier nodes credentials --write",
		"lint": "eslint nodes credentials package.json",
		"lintfix": "eslint nodes credentials package.json --fix",
		"prepublishOnly": "npm run build && npm run lint -s"
	},
	"files": [
		"dist"
	],
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/OracleTest.credentials.js"
		],
		"nodes": [
			"dist/nodes/OracleTest/OracleTest.node.js"
		]
	},
	"devDependencies": {
		"@types/node": "^18.16.16",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.42.0",
		"eslint-plugin-n8n-nodes-base": "^1.11.0",
		"gulp": "^4.0.2",
		"n8n-workflow": "*",
		"prettier": "^2.8.8",
		"typescript": "^5.1.3"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
